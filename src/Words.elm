amountOfWords : Int
amountOfWords =
    Array.length words


parseToWord : List String -> Word
parseToWord parts =
    case parts of
        word :: normalized :: dictionary :: _ ->
            Word
                word
                normalized
                (dictionary |> String.replace "''" "'" |> String.split "|")

        _ ->
            Word "" "" []


removeFirstAndLastCharacter =
    String.dropLeft 1 >> String.dropRight 1
